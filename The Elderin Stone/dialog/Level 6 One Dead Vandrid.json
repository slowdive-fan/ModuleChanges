                                                      "NodePortraitBitmap": "",
                                                      "NodeNpcName": "",
                                                      "NodeSound": "none",
                                                      "conversationText": "\"What is your business, Dalamar? What the hells are you really up to?\"",
                                                      "IsExpanded": true,
                                                      "subNodes": [
                                                        {
                                                          "idNum": 20,
                                                          "orderNum": 0,
                                                          "pcNode": false,
