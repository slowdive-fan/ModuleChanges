              "subNodes": [
                {
                  "idNum": 3,
                  "orderNum": 0,
                  "pcNode": false,
                  "linkTo": 0,
                  "ShowOnlyOnce": false,
                  "NodeIsActive": true,
                  "NodePortraitBitmap": "",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "You don't understand the spidery language the book is written in. Many of cracked, yellow pages include drawings of desparate looking people trapped in cages in a huge underground cavern.",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 4,
