              "ShowOnlyOnce": false,
              "NodeIsActive": true,
              "NodePortraitBitmap": "",
              "NodeNpcName": "",
              "NodeSound": "none",
              "conversationText": "\"Hiding? What, from you? Don't make me laugh.\"",
              "IsExpanded": true,
              "subNodes": [
                {
                  "idNum": 5,
                  "orderNum": 0,
                  "pcNode": false,
