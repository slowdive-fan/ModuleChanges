                  "NodeIsActive": true,
                  "NodePortraitBitmap": "",
                  "NodeNpcName": "",
                  "NodeSound": "none",
                  "conversationText": "A cry goes up the minute you slip around the corner of the temple. You have been seen!",
                  "IsExpanded": true,
                  "subNodes": [
                    {
                      "idNum": 7,
                      "orderNum": 0,
                      "pcNode": true,
