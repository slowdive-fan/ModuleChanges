                                                                                  "NodePortraitBitmap": "",
                                                                                  "NodeNpcName": "",
                                                                                  "NodeSound": "none",
                                                                                  "conversationText": "\"This is your last chance, <FullName>. I have no quarrel with you.\"",
                                                                                  "IsExpanded": true,
